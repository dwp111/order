package cn.tootoo.soa.oms.utils;

/**
 * 公式类型
 * @author zhangyu
 *
 */
public enum CalculateTypeForTHEnum {
	REVERSE_PRICE("0","反算单价"),
	GOODS_AMOUNT("1","计算商品总价"),
	REVERSE_GOODS_AMOUNT("2","反算商品总价");
	
	CalculateTypeForTHEnum(String c, String s){
		this.c = c;
		this.s = s;
	} 
	
	private String c;
	private String s;
	public String getC() {
		return c;
	}
	public void setC(String c) {
		this.c = c;
	}
	public String getS() {
		return s;
	}
	public void setS(String s) {
		this.s = s;
	}
	
	
	
}
